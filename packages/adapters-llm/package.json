{
  "name": "@noesis/adapters-llm",
  "version": "0.1.0",
  "description": "LLM integration adapters for Noesis SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "noesis",
    "llm",
    "openai",
    "anthropic",
    "learning"
  ],
  "author": "Noesis Team",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.98.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.27.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@noesis-edu/core": "*"
  },
  "peerDependenciesMeta": {
    "@noesis-edu/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
