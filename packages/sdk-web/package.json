{
  "name": "@noesis/sdk-web",
  "version": "0.1.0",
  "description": "Web-friendly facade for Noesis SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "noesis",
    "sdk",
    "learning",
    "web"
  ],
  "author": "Noesis Team",
  "license": "MIT",
  "dependencies": {
    "@noesis/adapters-attention-web": "*",
    "@noesis/adapters-llm": "*",
    "@noesis/core": "*"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
